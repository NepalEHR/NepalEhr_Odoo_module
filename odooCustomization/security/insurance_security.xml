<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <record model="ir.module.category" id="module_custom_report_category">
        <field name="name">Report</field>
        <field name="description">User access level to access report through Odoo</field>
        <field name="sequence">10</field>
    </record>
    
    <record id="group_custom_report_pharmacy" model="res.groups">
        <field name="name">Pharmacy</field>
        <field name="category_id" ref="module_custom_report_category"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
    </record>
        
    <record id="group_custom_report_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="module_custom_report_category"/>
        <field name="implied_ids" eval="[(4, ref('group_insurance_claim_user'))]"/>
    </record>
    
</odoo>